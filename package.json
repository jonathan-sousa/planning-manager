{
  "name": "planning-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "echo \"No tests configured yet\"",
    "analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next .amplify node_modules",
    "amplify:sandbox": "npx ampx sandbox",
    "amplify:sandbox:delete": "npx ampx sandbox delete",
    "amplify:generate": "npx ampx generate graphql-client-code",
    "amplify:deploy": "npx ampx pipeline-deploy",
    "precommit": "npm run lint:fix && npm run format && npm run typecheck",
    "prepare": "echo \"Husky setup skipped for now\""
  },
  "dependencies": {
    "aws-amplify": "^6.15.5",
    "next": "15.5.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.16.1",
    "@aws-amplify/backend-cli": "^1.8.0",
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "critters": "^0.0.25",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
